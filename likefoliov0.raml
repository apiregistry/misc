#%RAML 1.0
title: Likefolio Api Endpoints
version: v0
baseUri: likefolio.com/api
description: Create a session to start to use it
uses:
  commons: https://raw.githubusercontent.com/apiregistry/commons/master/commons.raml
  extras: https://raw.githubusercontent.com/apiregistry/typesExtras/master/typeExtras.raml
/v0:
  /brands:
    /mentionQty.json:
      get:
        displayName: Returns ordered mentions count for the given brand
        queryParameters:
          apiKey:
            type: string
            description: Api token granted to call the rest of the endpoints. You must call sessions/create to obtain a token and it will be valid for 15 minutes. After that, you must create another session.
            displayName: apiKey
          id:
            type: integer
            displayName: id
          startDate:
            type: datetime
            description: Start of the period. Must be in ISO8601 format
            displayName: startDate
          endDate?:
            type: datetime
            description: End of period. Must be in ISO8601 format
            displayName: endDate
        responses:
          200:
            description: No response was specified
    /show.json:
      get:
        displayName: Returns brands basic information (up tp 50)
        queryParameters:
          apiKey:
            type: string
            description: Api token granted to call the rest of the endpoints. You must call sessions/create to obtain a token and it will be valid for 15 minutes. After that, you must create another session.
            displayName: apiKey
          id:
            type: integer
            displayName: id
        responses:
          200:
            description: No response was specified
    /statistics.json:
      get:
        displayName: Returns statistics about the brand
        queryParameters:
          apiKey:
            type: string
            description: Api token granted to call the rest of the endpoints. You must call sessions/create to obtain a token and it will be valid for 15 minutes. After that, you must create another session.
            displayName: apiKey
          id:
            type: integer
            displayName: id
          period?:
            type: string
            description: 'Number of days/hours to get daya from. Period is calculated
              backwards from now. For example, 1day means today. 7days means past
              7 days and so on. The YTD value is the period from the beginning of
              the current year to now, different from 1year, which means the past
              365 days. Posible values: ["1hour", "6hours", "12hours", "24hours",
              "72hours", "120hours", "168hours", "240hours", "336hours", "1day", "3days",
              "5days", "7days", "10days", "14days", "1month", "3months", "6months",
              "9months", "YTD", "1year", "2years", "3years"]'
            displayName: period
        responses:
          200:
            description: No response was specified
    /topTweets.json:
      post:
        displayName: Returns list of tweets ordered by date
        responses:
          200:
            description: No response was specified
    /topUsers.json:
      get:
        displayName: Returns the list of Twitter IDs that have posted more mentions for the company
        queryParameters:
          apiKey:
