            displayName: startDate
          endDate?:
            type: datetime
            description: End of period. Must be in ISO8601 format
            displayName: endDate
          qty?:
            type: integer
            description: Quantity of records to return
            displayName: qty
        responses:
          200:
            description: No response was specified
