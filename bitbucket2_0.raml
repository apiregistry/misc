  Anonimous0:
    type: object
    properties:
      created_on?:
        type: datetime
      display_name?:
        type: string
      links?:
        type: object
        properties:
          followers?:
            type: object
            properties:
              href?:
                type: string
          repositories?:
            type: object
            properties:
              href?:
                type: string
          following?:
            type: object
            properties:
              href?:
                type: string
          self?:
            type: object
            properties:
              href?:
                type: string
          html?:
            type: object
            properties:
              href?:
                type: string
          avatar?:
            type: object
            properties:
              href?:
                type: string
      username?:
        type: string
        pattern: ^[a-zA-Z0-9_\-]+$
      uuid?:
        type: string
      website?:
        type: string
    description: An account object.
