/exports:
  /activity.json:
    post:
      description: 'Begins an export of your activity history. The activity will be
        exported to a zip archive containing a single file named activity.csv in the
        same format as you would be able to export from your account''s activity view.
        It includes the following fields: Date, Email Address, Sender, Subject, Status,
        Tags, Opens, Clicks, Bounce Detail. If you have configured any custom metadata
        fields, they will be included in the exported data.'
      responses:
        200:
          description: OK
          body:
            application/json:
              type: exports_satus
  /info.json:
    post:
      description: Returns information about an export job. If the export job's state is 'complete', the returned data will include a URL you can use to fetch the results. Every export job produces a zip archive, but the format of the archive is distinct for each job type. The api calls that initiate exports include more details about the output format for that job type.
      responses:
        200:
          description: OK
          body:
            application/json:
              type: exports_info-response
  /list.json:
    post:
      description: Returns a list of your exports.
      responses:
        200:
          description: OK
          body:
            application/json:
              type: exports_list-response
  /rejects.json:
    post:
      description: 'Begins an export of your rejection blacklist. The blacklist will
        be exported to a zip archive containing a single file named rejects.csv that
        includes the following fields: email, reason, detail, created_at, expires_at,
        last_event_at, expires_at.'
      responses:
        200:
          description: OK
          body:
            application/json:
              type: exports_satus
  /whitelist.json:
    post:
      description: 'Begins an export of your rejection whitelist. The whitelist will
        be exported to a zip archive containing a single file named whitelist.csv
        that includes the following fields: email, detail, created_at.'
      responses:
        200:
          description: OK
          body:
            application/json:
              type: exports_satus
/inbound:
  /add-domain.json:
    post:
      description: Add an inbound domain to your account
      responses:
        200:
          description: OK
          body:
            application/json:
              type: inbound_info
  /add-route.json:
    post:
      description: Add a new mailbox route to an inbound domain
      responses:
        200:
          description: OK
          body:
            application/json:
              type: route
  /check-domain.json:
    post:
      description: Check the MX settings for an inbound domain. The domain must have already been added with the add-domain call
      responses:
        200:
          description: OK
          body:
            application/json:
              type: inbound_info
  /delete-domain.json:
    post:
      description: Delete an inbound domain from the account. All mail will stop routing for this domain immediately.
      responses:
        200:
          description: OK
          body:
            application/json:
              type: inbound_info
  /delete-route.json:
    post:
      description: Delete an existing inbound mailbox route
      responses:
        200:
          description: OK
          body:
            application/json:
              type: route
  /domains.json:
    post:
      description: List the domains that have been configured for inbound delivery
      responses:
        200:
          description: OK
          body:
            application/json:
              type: inbound_domains-response
  /routes.json:
    post:
      description: List the mailbox routes defined for an inbound domain
      responses:
        200:
          description: OK
          body:
            application/json:
              type: inbound_routes-response
  /send-raw.json:
    post:
      description: Take a raw MIME document destined for a domain with inbound domains set up, and send it to the inbound hook exactly as if it had been sent over SMTP
      responses:
        200:
          description: OK
          body:
            application/json:
              type: inbound_send-raw-response
  /update-route.json:
    post:
      description: Update the pattern or webhook of an existing inbound mailbox route. If null is provided for any fields, the values will remain unchanged.
      responses:
        200:
          description: OK
          body:
            application/json:
              type: route
/ips:
  /cancel-warmup.json:
    post:
      description: Cancels the warmup process for a dedicated IP.
      responses:
        200:
          description: OK
          body:
            application/json:
              type: ip_info
  /check-custom-dns.json:
    post:
      description: Tests whether a domain name is valid for use as the custom reverse DNS for a dedicated IP.
      responses:
        200:
          description: OK
          body:
            application/json:
              type: ips_check-custom-dns-response
  /create-pool.json:
    post:
      description: Creates a pool and returns it. If a pool already exists with this name, no action will be performed.
      responses:
        200:
          description: OK
          body:
            application/json:
              type: ips_pool
  /delete-pool.json:
    post:
      description: Deletes a pool. A pool must be empty before you can delete it, and you cannot delete your default pool.
      responses:
        200:
          description: OK
          body:
            application/json:
              type: ips_delete-pool-response
  /delete.json:
    post:
      description: Deletes a dedicated IP. This is permanent and cannot be undone.
      responses:
        200:
          description: OK
          body:
            application/json:
              type: ips_delete-response
  /info.json:
    post:
      description: Retrieves information about a single dedicated ip.
      responses:
        200:
          description: OK
          body:
            application/json:
              type: ip_info
  /list-pools.json:
    post:
      description: Lists your dedicated IP pools.
      responses:
        200:
          description: OK
          body:
            application/json:
              type: ips_list-pools-response
  /list.json:
    post:
      description: Lists your dedicated IPs.
      responses:
        200:
          description: OK
          body:
            application/json:
              type: ips_list-response
  /pool-info.json:
    post:
      description: Describes a single dedicated IP pool.
      responses:
        200:
          description: OK
          body:
            application/json:
              type: ips_pool
  /provision.json:
    post:
      description: Requests an additional dedicated IP for your account. Accounts may have one outstanding request at any time, and provisioning requests are processed within 24 hours.
      responses:
        200:
          description: OK
          body:
            application/json:
              type: ips_provision-response
  /set-custom-dns.json:
    post:
      description: Configures the custom DNS name for a dedicated IP.
      responses:
        200:
          description: OK
          body:
            application/json:
              type: ip_info
  /set-pool.json:
    post:
      description: Moves a dedicated IP to a different pool.
      responses:
        200:
          description: OK
          body:
            application/json:
              type: ip_info
  /start-warmup.json:
    post:
      description: Begins the warmup process for a dedicated IP. During the warmup process, Mandrill will gradually increase the percentage of your mail that is sent over the warming-up IP, over a period of roughly 30 days. The rest of your mail will be sent over shared IPs or other dedicated IPs in the same pool.
      responses:
        200:
          description: OK
          body:
            application/json:
              type: ip_info
