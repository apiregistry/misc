  /info.json:
    post:
      description: Get the information for a single recently sent message
      responses:
        200:
          description: OK
          body:
            application/json:
              type: messages_info-response
  /list-scheduled.json:
    post:
      description: Queries your scheduled emails by sender or recipient, or both.
      responses:
        200:
          description: OK
          body:
            application/json:
              type: messages_list-scheduled-response
  /parse.json:
    post:
      description: Parse the full MIME document for an email message, returning the content of the message broken into its constituent pieces
      responses:
        200:
          description: OK
          body:
            application/json:
              type: messages_parse-response
  /reschedule.json:
