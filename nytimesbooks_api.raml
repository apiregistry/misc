/reviews.{format}:
  uriParameters:
    format:
      type: string
      displayName: format
      enum:
      - json
      - jsonp
  get:
    description: ''
    displayName: Reviews
    queryParameters:
      isbn?:
        type: integer
        description: Searching by ISBN is the recommended method. You can enter 10- or 13-digit ISBNs.
        displayName: isbn
      title?:
        type: string
        description: !!binary |-
          WW91w6LCgMKZbGwgbmVlZCB0byBlbnRlciB0aGUgZnVsbCB0aXRsZSBvZiB0aGUgYm9vay4gU3BhY2VzIGluIHRoZSB0aXRsZSB3aWxsIGJlIGNvbnZlcnRlZCBpbnRvIHRoZSBjaGFyYWN0ZXJzICUyMC4=
        displayName: title
      author?:
        type: string
        description: !!binary |-
          WW91w6LCgMKZbGwgbmVlZCB0byBlbnRlciB0aGUgYXV0aG9yw6LCgMKZcyBmaXJzdCBhbmQgbGFzdCBuYW1lLCBzZXBhcmF0ZWQgYnkgYSBzcGFjZS4gVGhpcyBzcGFjZSB3aWxsIGJlIGNvbnZlcnRlZCBpbnRvIHRoZSBjaGFyYWN0ZXJzICUyMC4=
        displayName: author
      api-key?:
        type: string
        displayName: api-key
    responses:
      200:
        description: ''
        body:
          application/json:
            type: object
            properties:
              copyright?:
                type: string
              results?:
                type: array
                items:
                  type: object
                  properties:
                    summary?:
                      type: string
                    book_title?:
                      type: string
                    isbn13?:
                      type: array
                      items:
                        type: string
                    book_author?:
                      type: string
                    byline?:
                      type: string
                    publication_dt?:
                      type: string
                    url?:
                      type: string
              num_results?:
                type: integer
              status?:
                type: string
