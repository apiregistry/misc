/publishers:
  /{publisher_id}:
    uriParameters:
      publisher_id:
        type: integer
        description: Publisher ID
        displayName: publisher_id
        format: int64
    /account:
      get:
        description: Returns account, contact, business, financial & payment information
        displayName: Get publisher account information
        responses:
          200:
            description: No response was specified
            body:
              application/json:
                type: publisher_account
          404:
            description: Publisher not found
    /clicks:
      get:
        description: Returns a set of click
        displayName: Get all raw click data
        queryParameters:
          start:
            type: datetime
            description: Start date (YYYY-MM-DD)
            displayName: start
          end:
            type: datetime
            description: End date (YYYY-MM-DD)
            displayName: end
          page?:
            type: integer
            description: Paginator page
            displayName: page
            default: 1
            format: int64
