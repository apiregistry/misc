#%RAML 1.0
title: Daisycon
version: 1.0.0
baseUri: services.daisycon.com/
description: The Daisycon API allows you to retrieve statistics and link materials, update transactions, subscribe to affiliate program, and a whole lot more!
uses:
  commons: https://raw.githubusercontent.com/apiregistry/commons/master/commons.raml
  extras: https://raw.githubusercontent.com/apiregistry/typesExtras/master/typeExtras.raml
types:
  ad:
    type: object
    properties:
      ad_url?:
        type: string
        description: Url where this ad is hosted
      adgroup_id?:
        type: integer
      click_url?:
        type: string
        description: Tracking/deeplink url for this ad
      content?:
        type: string
      height?:
        type: integer
      id?:
        type: integer
      last_modified?:
        type: datetime
      type?:
        type: string
        description: This is the mime type of the ad
        enum:
        - image/jpeg
        - image/jpg
        - image/png
        - image/gif
        - text/html
        - text/plain
        - application/x-shockwave-flash
      width?:
        type: integer
  address:
