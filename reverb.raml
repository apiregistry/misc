/cart:
  /move_to_watch_list:
    /{cart_item_id}:
      uriParameters:
        cart_item_id:
          type: string
          displayName: cart_item_id
      post:
        description: Remove a cart item and add it to watch list
        displayName: Remove a cart item and add it to watch list
        responses:
          500:
            description: Unexpected error
  /{cart_item_id}:
    uriParameters:
      cart_item_id:
        type: string
        displayName: cart_item_id
    put:
      description: Update cart item details
      displayName: Update cart item details
      body:
        application/json:
          type: object
          properties:
            email:
              type: string
              description: Email address.
            first_name:
              type: string
              description: First Name
            last_name:
              type: string
              description: Last Name
            newsletter_opt_in?:
              type: boolean
              description: Subscribe to gear deals by email
            password:
              type: string
              description: Password (at least 8 characters).
          description: the content of the request
      responses:
        500:
          description: Unexpected error
  /{product_id}:
    uriParameters:
      product_id:
        type: string
        displayName: product_id
    delete:
      description: Remove a product from the cart
      displayName: Remove a product from the cart
      responses:
        500:
          description: Unexpected error
