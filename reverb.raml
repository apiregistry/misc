/handpicked:
  /{slug}:
    uriParameters:
      slug:
        type: string
        displayName: slug
    get:
      description: Get results from a handpicked collection
      displayName: Get results from a handpicked collection
      queryParameters:
        query?:
          type: string
          description: Search query.
          displayName: query
        auction_price_max?:
          type: number
          description: Maximum current auction price
          displayName: auction_price_max
          format: float
        category?:
          type: string
          description: Category slug from /api/categories
          displayName: category
        product_type?:
          type: string
          description: Product type slug from /api/categories
          displayName: product_type
        condition?:
          type: string
          description: 'Condition: new,used,all'
          displayName: condition
        decade?:
          type: string
          description: 'Decade: e.g. 1970s, early 70s'
          displayName: decade
        finish?:
          type: string
          description: Visual finish of the item, common for guitars
          displayName: finish
        handmade?:
          type: boolean
          description: Handmade items only
          displayName: handmade
        item_city?:
          type: string
          description: City where item is located
          displayName: item_city
        item_country?:
          type: string
          description: Country code where item is located
          displayName: item_country
        item_state?:
          type: string
          description: State or region code where item is located
          displayName: item_state
        make?:
          type: string
          description: Make(s)/brand of item (e.g. Fender). Can take a single value or an array.
          displayName: make
        model?:
          type: string
          description: Model of item (e.g. Stratocaster)
          displayName: model
        must_not?:
          type: string
          description: Search term negation. If you want to exclude a term, add it here
          displayName: must_not
        price_max?:
          type: number
          description: Maximum price of search results (USD)
          displayName: price_max
          format: float
        price_min?:
          type: number
          description: Minimum price of search results (USD)
          displayName: price_min
          format: float
        year_max?:
          type: integer
          description: Maximum year of manufacture
          displayName: year_max
        year_min?:
          type: integer
          description: Minumum year of manufacture
          displayName: year_min
        accepts_gift_cards?:
          type: boolean
          description: If true, include only items that accept gift cards
          displayName: accepts_gift_cards
        preferred_seller?:
          type: boolean
          description: If true, include only items by Reverb Preferred Sellers
          displayName: preferred_seller
        shop?:
          type: string
          description: Slug of shop to search
          displayName: shop
        shop_id?:
          type: string
          description: ID of shop to search
          displayName: shop_id
        listing_type?:
          type: string
          description: 'Type of listing: auction,make_an_offer,buy_it_now'
          displayName: listing_type
        ships_to?:
          type: string
          description: Limit search to items that ship to this country code
          displayName: ships_to
        exclude_auctions?:
          type: boolean
          description: If true, exclude auctions
          displayName: exclude_auctions
        accepts_payment_plans?:
          type: boolean
          description: If true, only show items that can be purchased with a payment plan
          displayName: accepts_payment_plans
        watchers_count_min?:
          type: integer
          description: Minimum number of watchers (used to find popular items)
          displayName: watchers_count_min
        page?:
          type: integer
          displayName: page
          default: 1
        per_page?:
          type: integer
          displayName: per_page
          default: 24
      responses:
        500:
          description: Unexpected error
