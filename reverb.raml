#%RAML 1.0
title: reverb
version: '2.0'
baseUri: reverb.com/api
description: reverb
mediaType:
- application/json
uses:
  commons: https://raw.githubusercontent.com/apiregistry/commons/master/commons.raml
  extras: https://raw.githubusercontent.com/apiregistry/typesExtras/master/typeExtras.raml
securitySchemes:
  oauth2:
    type: OAuth 2.0
    settings:
      authorizationGrants:
      - authorization_code
      authorizationUri: https://reverb.com/oauth/authorize
      accessTokenUri: https://reverb.com/oauth/token
      scopes:
      - update_listings
      - write
/accounts:
  post:
    description: Create an account.
    displayName: Create an account.
    body:
      application/json:
        type: object
        properties:
          email:
            type: string
            description: Email address.
          first_name:
            type: string
            description: First Name
          last_name:
            type: string
            description: Last Name
          newsletter_opt_in?:
            type: boolean
            description: Subscribe to gear deals by email
          password:
            type: string
            description: Password (at least 8 characters).
        description: the content of the request
    responses:
      500:
        description: Unexpected error
/addresses:
  /{address_id}:
    uriParameters:
      address_id:
        type: string
        displayName: address_id
    delete:
      description: Delete an existing address in your address book
      displayName: Delete an existing address in your address book
      responses:
        500:
          description: Unexpected error
    put:
      description: Update an existing address in your address book
      displayName: Update an existing address in your address book
      body:
        application/json:
          type: object
          properties:
            email:
              type: string
              description: Email address.
            first_name:
              type: string
              description: First Name
            last_name:
              type: string
              description: Last Name
            newsletter_opt_in?:
              type: boolean
              description: Subscribe to gear deals by email
            password:
              type: string
              description: Password (at least 8 characters).
          description: the content of the request
      responses:
        500:
          description: Unexpected error
  get:
    description: See all addresses in your address book
    displayName: See all addresses in your address book
    responses:
      500:
        description: Unexpected error
  post:
    description: Create a new address in your address book
    displayName: Create a new address in your address book
    body:
      application/json:
        type: object
        properties:
          email:
            type: string
            description: Email address.
          first_name:
            type: string
            description: First Name
          last_name:
            type: string
            description: Last Name
          newsletter_opt_in?:
            type: boolean
            description: Subscribe to gear deals by email
          password:
            type: string
            description: Password (at least 8 characters).
        description: the content of the request
    responses:
      500:
        description: Unexpected error
/articles:
  /featured:
    get:
      description: See featured Reverb blog posts
      displayName: See featured Reverb blog posts
      responses:
        500:
          description: Unexpected error
  /{slug}:
    uriParameters:
      slug:
        type: string
        displayName: slug
    /related-listings:
      get:
        description: Find listings related to an article
        displayName: Find listings related to an article
        responses:
          500:
            description: Unexpected error
    get:
      description: Display a single article
      displayName: Display a single article
      responses:
        500:
          description: Unexpected error
  get:
    description: See all Reverb blog posts
    displayName: See all Reverb blog posts
    queryParameters:
      exclude_featured?:
        type: integer
        description: Number of featured articles to exclude
        displayName: exclude_featured
    responses:
      500:
        description: Unexpected error
