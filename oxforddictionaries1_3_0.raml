/wordlist:
  /{source_lang}:
    uriParameters:
      source_lang:
        type: string
        description: IANA language code
        displayName: source_lang
        enum:
        - en
        - es
        - lv
        - hi
    /{filters_advanced}:
      uriParameters:
        filters_advanced:
          type: string
          description: 'Semicolon separated list of wordlist parameters, presented
            as value pairs: LexicalCategory, domains, regions, registers. Parameters
            can take comma separated list of values. E.g., lexicalCategory=noun,adjective;domains=sport'
          displayName: filters_advanced
          default: registers=Rare;domains=Art
      get:
        description: "Advanced options for retrieving wordlist - exclude filter, filter\
          \ by word length or match by substring (prefix). \n\n  <div id=\"wordlist_advanced\"\
          ></div>\n"
        displayName: Retrieve list of words for category with advanced options.
        queryParameters:
          exclude?:
            type: string
            description: 'Semicolon separated list of wordlist parameters to be excluded
              from response, presented as value pairs: LexicalCategory, domains, regions,
              registers. Parameters can take comma separated list of values.'
            displayName: exclude
          word_length?:
            type: string
            description: Parameter to speficy the minimum (>), exact or maximum (<) length of the words required. E.g., >5 - more than 5 chars; <4 - less than 4 chars; >5<10 - from 5 to 10 chars; 3 - exactly 3 chars.
            displayName: word_length
          prefix?:
            type: string
            description: Filter words that start with prefix parameter
            displayName: prefix
            default: eye
          exact?:
            type: boolean
            description: If exact=true wordlist returns a list of entries that exactly matches the search string. Otherwise wordlist lists entries that start with prefix string.
            displayName: exact
          limit?:
            type: string
            description: Limit the number of results per response. Default and maximum limit is 5000.
            displayName: limit
          offset?:
            type: string
            description: Offset the start number of the result.
            displayName: offset
        headers:
          app_id:
            type: string
            description: App ID Authentication Parameter
            displayName: app_id
            default: 5037d509
          app_key:
            type: string
            description: App Key Authentication Parameter
            displayName: app_key
            default: 4dc1aebaa63721f0f8e79a55e2514bc7
        responses:
          200:
            description: Successful response.
            body:
              application/json:
                type: Wordlist
                displayName: WordlistResults
          400:
            description: disjoint sets of word_length values (eg <3>10); filter has no values; total number of filters > 20; filters and excludes values can not be the same; source_lang and translations can not be same; word_length conflicts with prefix length.
          404:
            description: |
              No entries is found matching supplied filters or lang or filters is not recognized.
          500:
            description: |
              Internal Error. An error occourred while processing the data.
    /{filters_basic}:
      uriParameters:
        filters_basic:
          type: string
          description: 'Semicolon separated list of wordlist parameters, presented
            as value pairs: LexicalCategory, domains, regions, registers. Parameters
