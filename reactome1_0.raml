          description: pathways per page
          displayName: pageSize
          default: 20
          format: int32
        page?:
          type: integer
          description: page number
          displayName: page
          default: 1
          format: int32
        sortBy?:
          type: string
          description: how to sort the result
          displayName: sortBy
          default: ENTITIES_PVALUE
          enum:
          - NAME
          - TOTAL_ENTITIES
          - TOTAL_INTERACTORS
          - TOTAL_REACTIONS
          - FOUND_ENTITIES
          - FOUND_INTERACTORS
          - FOUND_REACTIONS
          - ENTITIES_RATIO
          - ENTITIES_PVALUE
          - ENTITIES_FDR
          - REACTIONS_RATIO
        order?:
          type: string
          description: specifies the order
          displayName: order
          default: ASC
          enum:
          - ASC
          - DESC
        resource?:
          type: string
          description: the resource to sort
          displayName: resource
          default: TOTAL
          enum:
          - TOTAL
          - UNIPROT
          - ENSEMBL
          - CHEBI
          - MIRBASE
          - NCBI_PROTEIN
