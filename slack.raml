            type: groups
/groups.mark:
  get:
    description: Sets the read cursor in a private group.
    queryParameters:
      channel:
        type: string
        description: Group to fetch history for.
        displayName: channel
      ts:
        type: string
        description: Timestamp of the most recently seen message.
        displayName: ts
    responses:
      200:
        description: OK
        body:
          application/json:
            type: ok
/groups.setPurpose:
  get:
    description: Sets the purpose for a private group.
    queryParameters:
      channel:
