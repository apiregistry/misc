              creator?:
                type: string
              value?:
                type: string
          created?:
            type: number
          unread_count_display?:
            type: number
          unread_count?:
            type: number
          is_archived?:
            type: boolean
          is_channel?:
            type: boolean
          is_general?:
            type: boolean
          members?:
            type: array
            items:
              type: string
          name?:
            type: string
          topic?:
            type: object
            properties:
              last_set?:
                type: number
              creator?:
                type: string
              value?:
                type: string
          id?:
            type: string
          latest?:
            type: object
            properties:
              attachments?:
