/auth.test:
  get:
    description: Checks authentication and tells you who you are.
    queryParameters:
      token:
        type: string
        description: Authentication token
        displayName: token
    responses:
      200:
        description: OK
        body:
          application/json:
            type: user
/channels.history:
  get:
    description: Fetches history of messages and events from a channel.
    queryParameters:
      channel:
        type: string
        description: Channel to fetch history for.
        displayName: channel
      latest?:
        type: string
        description: Timestamp of the oldest recent seen message.
        displayName: latest
        default: now
      oldest?:
        type: string
        description: Timestamp of the latest previously seen message.
        displayName: oldest
      count?:
        type: number
        description: Number of messages to return, between 1 and 1000.
        displayName: count
        default: 100.0
      has_more?:
        type: boolean
        displayName: has_more
      is_limited?:
        type: boolean
        displayName: is_limited
    responses:
      200:
        description: OK
        body:
          application/json:
            type: channelsHistory
/channels.info:
  get:
    description: Gets information about a channel.
    queryParameters:
      channel:
        type: string
        description: Channel to fetch history for.
        displayName: channel
    responses:
      200:
        description: OK
        body:
          application/json:
            type: searchAll
/channels.invite:
  get:
    description: Invites a user to a channel.
    queryParameters:
      channel:
        type: string
        description: Channel to fetch history for.
        displayName: channel
      user:
        type: string
        description: User to invite to channel.
        displayName: user
    responses:
      200:
        description: OK
        body:
          application/json:
            type: channelsInvite
/channels.join:
  get:
    description: Joins a channel, creating it if needed.
    queryParameters:
      name:
        type: string
        description: Name of channel to join
        displayName: name
    responses:
      200:
        description: OK
        body:
          application/json:
            type: channelsInvite
/channels.leave:
  get:
    description: Leaves a channel.
    queryParameters:
      channel:
        type: string
        description: Channel to fetch history for.
        displayName: channel
    responses:
      200:
        description: OK
        body:
          application/json:
            type: ok
/channels.list:
  get:
    description: Lists all channels in a Slack team.
    queryParameters:
      exclude_archived?:
        type: string
        description: Don't return archived channels.
        displayName: exclude_archived
    responses:
      200:
        description: OK
        body:
          application/json:
            type: channels
/channels.mark:
  get:
    description: Sets the read cursor in a channel.
    queryParameters:
      channel:
        type: string
        description: Channel to fetch history for.
        displayName: channel
      ts:
        type: string
        description: Timestamp of the most recently seen message.
        displayName: ts
    responses:
      200:
        description: OK
        body:
          application/json:
            type: ok
/channels.setPurpose:
  get:
    description: Sets the purpose for a channel.
    queryParameters:
      channel:
        type: string
        description: Channel to fetch history for.
        displayName: channel
      purpose:
        type: string
        description: The new purpose
        displayName: purpose
    responses:
      200:
        description: OK
        body:
          application/json:
            type: purpose
/channels.setTopic:
  get:
    description: Sets the topic for a channel.
    queryParameters:
      channel:
        type: string
        description: Channel to fetch history for.
        displayName: channel
      topic:
        type: string
        description: The new topic
        displayName: topic
    responses:
      200:
        description: OK
        body:
          application/json:
            type: topic
/chat.delete:
  get:
    description: Deletes a message.
    queryParameters:
      channel:
        type: string
        description: Channel to fetch history for.
        displayName: channel
      ts:
        type: string
        description: Timestamp of the most recently seen message.
        displayName: ts
    responses:
      200:
        description: OK
        body:
          application/json:
            type: chatOk
/chat.postMessage:
  get:
    description: Sends a message to a channel.
    queryParameters:
      channel:
        type: string
        description: Channel to fetch history for.
        displayName: channel
      text:
        type: string
        description: Text of the message to send. See below for an explanation of formatting.
        displayName: text
      username?:
        type: string
        description: Name of bot.
        displayName: username
      parse?:
        type: string
        description: Change how messages are treated. See below.
        displayName: parse
      link_names?:
        type: string
        description: Find and link channel names and usernames.
        displayName: link_names
      unfurl_links?:
        type: string
        description: Pass 1 to enable unfurling of primarily text-based content.
        displayName: unfurl_links
      icon_url?:
        type: string
        description: URL to an image to use as the icon for this message
        displayName: icon_url
      icon_emoji?:
        type: string
        description: emoji to use as the icon for this message. Overrides icon_url.
        displayName: icon_emoji
      attachments?:
        type: string
        description: Structured message attachments.
        displayName: attachments
    responses:
      200:
        description: OK
        body:
          application/json:
            type: chatOk
/chat.update:
  get:
    description: Updates a message.
    queryParameters:
      channel:
        type: string
        description: Channel to fetch history for.
        displayName: channel
      text:
        type: string
        description: Text of the message to send. See below for an explanation of formatting.
        displayName: text
      ts:
        type: string
        description: Timestamp of the most recently seen message.
        displayName: ts
    responses:
      200:
        description: OK
        body:
          application/json:
            type: chatUpdate
/emoji.list:
  get:
    description: Lists custom emoji for a team.
    responses:
      200:
        description: OK
        body:
          application/json:
            type: emojiList
/files.info:
  get:
    description: Gets information about a team file.
    queryParameters:
      file:
        type: string
        description: File to fetch info for
        displayName: file
      count?:
        type: number
        description: Number of items to return per page.
        displayName: count
        default: 100.0
      page?:
        type: number
        description: Page number of results to return.
        displayName: page
        default: 1.0
    responses:
      200:
        description: OK
        body:
          application/json:
            type: fileObj
/files.list:
  get:
    description: Lists and filters team files.
    queryParameters:
      file:
        type: string
        description: File to fetch info for
        displayName: file
      user?:
        type: string
        description: Filter files created by a single user.
        displayName: user
      ts_from?:
        type: string
        description: Filter files created after this timestamp (inclusive).
        displayName: ts_from
      ts_to?:
        type: string
        description: Filter files created before this timestamp (inclusive).
        displayName: ts_to
        default: now
      types?:
        type: string
        description: You can pass multiple values in the types argument, like types=posts,snippets.The default value is all, which does not filter the list.
        displayName: types
        enum:
        - pdfs
        - zips
        - posts
        - images
        - snippets
        - gdocs
        - all
    responses:
      200:
        description: OK
        body:
          application/json:
            type: files
/files.upload:
  get:
    description: Uploads or creates a file.
    queryParameters:
      file?:
        type: string
        description: File contents via multipart/form-data.
        displayName: file
      content?:
        type: string
        description: File contents via a POST var.
        displayName: content
      filetype?:
        type: string
        description: Slack-internal file type identifier.
        displayName: filetype
      filename?:
        type: string
        description: Filename of file.
        displayName: filename
      title?:
        type: string
        description: Title of file.
        displayName: title
      initial_comment?:
        type: string
        description: Initial comment to add to file.
        displayName: initial_comment
      channels?:
        type: string
        description: Comma separated list of channels to share the file into.
        displayName: channels
    responses:
      200:
        description: OK
        body:
          application/json:
            type: fileObj
/groups.history:
