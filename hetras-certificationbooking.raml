#%RAML 1.0
title: hetras Booking API Version 0
version: v0
baseUri: api.hetras-certification.net/
uses:
  commons: https://raw.githubusercontent.com/apiregistry/commons/master/commons.raml
  extras: https://raw.githubusercontent.com/apiregistry/typesExtras/master/typeExtras.raml
types:
  AcceptedGuaranteeTypes:
    type: object
    properties:
      accepted?:
        type: array
        description: "In this list you find all the accepted guarantee types for this\
          \ offer. They are sorted in ascending\r\n            order."
        items:
          type: string
          enum:
          - PM4Hold
          - PM6Hold
          - GuaranteeToCreditCard
          - GuaranteeToGuestAccount
          - GuaranteeByTravelAgent
          - GuaranteeByCompany
          - Deposit
          - Voucher
          - Prepayment
          - NonGuaranteed
          - Tentative
          - Waitlist
      minimum?:
        type: string
        description: "Based on the rateplan a reservation does need to have a minimum\
          \ guarantee. When you create a new booking you\r\n            can always\
          \ use a higher guarantee type starting from the minimum. If you do not specify\
          \ a guarantee when creating\r\n            a new booking using this offer\
          \ this guarantee type will be used by default. See \r\n            https://developer.hetras.com/docs/tutorials#payment\
          \ for information about guarantee types and payment details"
        enum:
        - PM4Hold
        - PM6Hold
        - GuaranteeToCreditCard
        - GuaranteeToGuestAccount
        - GuaranteeByTravelAgent
        - GuaranteeByCompany
        - Deposit
        - Voucher
        - Prepayment
        - NonGuaranteed
        - Tentative
        - Waitlist
  AddonOffer:
    type: object
    properties:
      breakdown?:
        type: array
        description: "In this collection you will get an entry with price information\
          \ for every day the addon service will be charged\r\n            to the\
          \ folio of the reservation if the addon service is booked."
        items:
          type: AddonOfferBreakdown
      code?:
        type: string
        description: The code of the addon service
      description?:
        type: string
        description: The description of the addon service
      frequency?:
        type: string
        description: "The frequency this addon service will be charged to the reservation.\
          \ This field is a string that can be displayed,\r\n            but is not\
          \ supposed to be used for computation"
      name?:
        type: string
        description: The name of the addon service
      rate_mode?:
        type: string
        description: "The price for an addon service can be per person or per room.\
          \ All the prices in an offer are already calculated\r\n            for all\
          \ rooms and number of persons per room. Based on this attribute you will\
          \ be able to know how to calculate\r\n            the price per person and\
          \ room, per room or per person depending on your needs."
        enum:
        - PerRoom
        - PerPerson
      total_stay?:
        type: AddonOfferRate
        description: "This object gives you the price information if this addon service\
          \ is booked for the total stay defined in your\r\n            request by\
          \ arrivalDate and departureDate"
  AddonOfferBreakdown:
    type: object
    properties:
      business_day?:
        type: datetime
        description: The business day the price for the addon service will be posted to the folio of the reservation
      currency?:
        type: string
        description: The currency the addon service will be charged in.
      included_tax?:
        type: number
        description: "The amount of taxes already included in the gross rate of the\
          \ addon service also calculated for all rooms and\r\n            all persons."
        format: double
      rate?:
        type: number
        description: The gross rate of the addon service. It is the price calculated for all rooms and all persons.
        format: double
  AddonOfferRate:
    type: object
    properties:
      currency?:
        type: string
        description: The currency the addon service will be charged in.
      included_tax?:
        type: number
        description: "The amount of taxes already included in the gross rate of the\
          \ addon service also calculated for all rooms and\r\n            all persons."
        format: double
      rate?:
        type: number
        description: The gross rate of the addon service. It is the price calculated for all rooms and all persons.
        format: double
  Addons:
    type: object
    properties:
      addon_services?:
        type: array
        description: A collection of offers for addon services.
        items:
          type: AddonOffer
      adults?:
        type: integer
        description: The number of adults per room the addon offers are requested for. The rates will then already reflects this.
        format: int32
      arrival_date?:
        type: datetime
        description: The arrival date of the reservation the addon offers are supposed to be booked on.
      departure_date?:
        type: datetime
        description: The departure date of the reservation the addon offers are supposed to be booked on.
      hotel_id?:
        type: integer
        description: The id of the hotel the addon offers will be valid for
        format: int32
      hotel_name?:
        type: string
        description: The name of the hotel the addon offers will be valid for
      rate_plan?:
        type: RatePlan
        description: "The rate plan all the addon offers are valid for. The reservation\
          \ you want to book this addons for needs\r\n            to have the same\
          \ rate plan otherwise you might see different prices on the target reservation."
      room?:
        type: ReservationRoom
        description: "The room type all the addon offers are valid for. The reservation\
          \ you want to book this addons for needs\r\n            to have the same\
          \ room type otherwise you might see different prices on the target reservation."
      rooms?:
        type: integer
        description: The number of rooms the addon offers are requested for. The rates will then already reflects this.
        format: int32
  AssignRoomCriteria:
    type: object
    properties:
      amenities?:
        type: array
        description: Ensure the assigned room will have all the amenities specified. You can provide a comma seperated list of amenity codes.
        items:
          type: string
      condition?:
        type: string
        description: "Here you can define to limit the list of assignable rooms based\
          \ on their current condition. This is only applicable if the underlying\
          \ reservation\r\n            is due to arrive on the current business day.\
          \ If not set by default only clean rooms will be assigned."
        enum:
        - CleanNotInspected
        - Clean
        - Dirty
        - Any
      include_out_of_service?:
        type: boolean
        description: "Sometimes you might want to assign rooms which are out of service\
          \ (small repair needed) if no other rooms are available anymore. If you\
          \ set\r\n            include_out_of_service to true even those rooms will\
          \ be considered. The default is false."
      locations?:
        type: array
        description: Ensure the assigned room will have at least one of the specified locations. You can provide a comma seperated list of location codes.
        items:
          type: string
      respect_guest_preferences?:
        type: boolean
        description: "Defines if the preferences for locations, amenities and views\
          \ of the primary guest should be taken into account. All defined preferences\
          \ in the guest\r\n            profile override any of the criteria defined\
          \ in the request body. The default is false."
      room_number?:
        type: string
        description: "If you define a specific room number this room will be assigned\
          \ if not assigned to another reservation, has proper room type and is not\
          \ OutOfOrder \r\n            or OutOfInventory for the stay duration of\
          \ the underlying reservaton. If set all other filter criteria will be ignored."
      views?:
        type: array
        description: Ensure the assigned room will have at least one of the specified views. You can provide a comma seperated list of view codes.
        items:
          type: string
    description: Specifies the criteria for the room selection.
  AssignRoomResponse:
    type: object
    properties:
      _warnings?:
        type: array
        description: "Warnings that came up when your request was processed. Your\
          \ request will still be processed successfull when\r\n            you see\
          \ such warnings in your response"
        items:
          type: string
      room_number?:
        type: string
        description: The room number assigned
    description: Defines the response for room assignment operation
  AvailabilityDetail:
    type: object
    properties:
      _links?:
        type: object
        properties:
          //:
            type: LinkObject
        description: Collection of links to related resources
      available?:
        type: integer
        description: "The number of rooms that were originally available to sell.\
          \ This is the the house count reduced by rooms set\r\n            to OutOfOrder\
          \ but increased by the set overbooking"
        format: int32
      blocked?:
        type: Blocked
        description: The breakdown for rooms blocked
      day_use?:
        type: integer
        description: The number of day use reservations
        format: int32
      house_count?:
        type: integer
        description: The total count of physical rooms reduced by the number of rooms set to OutOfInventory
        format: int32
      maintenance?:
        type: Maintenance
        description: The breakdown for rooms under maintenance
      overbooking?:
        type: integer
        description: The manually set overbooking
        format: int32
      room_count?:
        type: integer
        description: The total count of physical rooms
        format: int32
      sold?:
        type: integer
        description: The count of rooms sold. It sums up the rooms sold through individual reservations plus rooms blocked definitely
        format: int32
      to_sell?:
        type: integer
        description: The number of rooms still available to sell. It is available reduced by the already sold rooms
        format: int32
  AvailabilityResponse:
    type: object
    properties:
      _count?:
        type: integer
        description: The number of items matching your request in total for all pages.
        format: int32
      _links?:
        type: object
        properties:
          //:
            type: LinkObject
        description: Collection of links to related resources
      daily_availabilities?:
        type: array
        description: The list of availability numbers per business day
        items:
          type: DailyAvailability
      hotel?:
        type: EmbeddedHotel
        description: The hotel info you requsted the availability numbers for
  BaseResponse:
    type: object
    properties:
      _warnings?:
        type: array
        description: "Warnings that came up when your request was processed. Your\
          \ request will still be processed successfull when\r\n            you see\
          \ such warnings in your response"
        items:
          type: string
  Block:
    type: object
    properties:
      _links?:
        type: object
        properties:
          //:
            type: LinkObject
        description: Collection of links to related resources
      average_daily_rate?:
        type: number
        description: Average daily rate for this block. You can multiply by the count of blocked room nights and get a forecast value for this block
        format: double
      code?:
        type: string
        description: "The code of a block. The code is composed of the hotel code\
          \ a dash and the block code shown in the hetras UI. \r\n            An example\
          \ block code on the API for a block with code TRAVEL1 defined for the hotel\
          \ with code ABC \r\n            would be ABC-TRAVEL1"
      count?:
        type: integer
        description: Total number of room nights blocked across all room types and days
        format: int32
      first_arrival?:
        type: datetime
        description: (Earliest) arrival date for reservations split from this block
      group?:
        type: GroupMaster
        description: Group master template linked to this block
      hotel?:
        type: HotelSummary
        description: Hotel information for the hotel the rooms are blocked for
      last_departure?:
        type: datetime
        description: (Latest) departure date for reservations split from this block
      name?:
        type: string
        description: The name of the block
      picked?:
        type: integer
        description: Total number of room nights picked up accross all room types and days
        format: int32
      rate_plans?:
        type: array
        description: The rateplans linked to this block
        items:
          type: BlockRatePlan
      status?:
        type: string
        description: The current status of the block
        enum:
        - Physical
        - Cancelled
        - Tentative
        - Definite
  BlockInfo:
    type: object
    properties:
      _links?:
        type: object
        properties:
          //:
            type: LinkObject
        description: Collection of links to related resources
      code?:
        type: string
        description: "The code is composed of the hotel code a dash and the block\
          \ code shown in the hetras UI. An \r\n            example block code on\
          \ the API for a block with code TRAVEL1 defined for the hotel with code\
          \ ABC would \r\n            be ABC-TRAVEL1"
      name?:
        type: string
        description: The name of the block
  BlockListRequest:
    type: object
    properties:
      from?:
        type: datetime
        description: Return all blocks where the block's last_departure is greater than specified date.
      groupCode?:
        type: string
        description: Filter the blocks by the specified group code
      hotelId?:
        type: integer
        description: Only return blocks for this specific hotel.
        format: int32
      ratePlanCodes?:
        type: array
        description: Return all blocks that have related the specified comma-separated rate plans.
        items:
          type: string
      status?:
        type: string
        description: Return all blocks where the block status is one of the specified values.
        enum:
        - Cancelled
        - Tentative
        - Definite
      to?:
        type: datetime
        description: Return all blocks where the block's last_departure is less than specified date.
  BlockRatePlan:
    type: object
    properties:
      code?:
        type: string
        description: Code of the rate plan
      description?:
        type: string
        description: Description of the rate plan suitable for being displayed to customers
      forecast?:
        type: boolean
        description: Speficies if this is the rateplan that should be used for calculating the forecast
      name?:
        type: string
        description: Name of the rate plan
  BlockResponse:
    type: object
    properties:
      _links?:
        type: object
        properties:
          //:
            type: LinkObject
        description: Collection of links to related resources
      average_daily_rate?:
        type: number
        description: Average daily rate for this block. You can multiply by the count of blocked room nights and get a forecast value for this block
        format: double
      code?:
        type: string
        description: "The code of a block. The code is composed of the hotel code\
          \ a dash and the block code shown in the hetras UI. \r\n            An example\
          \ block code on the API for a block with code TRAVEL1 defined for the hotel\
          \ with code ABC \r\n            would be ABC-TRAVEL1"
      count?:
        type: integer
        description: Total number of room nights blocked across all room types and days
        format: int32
      first_arrival?:
        type: datetime
        description: (Earliest) arrival date for reservations split from this block
      group?:
        type: GroupMaster
        description: Group master template linked to this block
      hotel?:
        type: HotelSummary
        description: Hotel information for the hotel the rooms are blocked for
      last_departure?:
        type: datetime
        description: (Latest) departure date for reservations split from this block
      name?:
        type: string
        description: The name of the block
      picked?:
        type: integer
        description: Total number of room nights picked up accross all room types and days
        format: int32
      rate_plans?:
        type: array
        description: The rateplans linked to this block
        items:
          type: BlockRatePlan
      room_type_counts?:
        type: array
        description: Number of blocked and picked up rooms per room type and day.
        items:
          type: RoomTypeCount
      status?:
        type: string
        description: The current status of the block
        enum:
        - Physical
        - Cancelled
        - Tentative
        - Definite
  BlockRoomType:
    type: object
    properties:
      _links?:
        type: object
        properties:
          //:
            type: LinkObject
        description: Collection of links to related resources
      code?:
        type: string
        description: Code of the room type
      name?:
        type: string
        description: Name of the room type
  Blocked:
    type: object
    properties:
      definite?:
        type: integer
        description: Number of rooms blocked defintely
        format: int32
      remaining?:
        type: integer
        description: Number of definitely blocked rooms not picked up
        format: int32
      tentative?:
        type: integer
        description: Number of rooms blocked tentatively
        format: int32
  BlocksResponse:
    type: object
    properties:
      _count?:
        type: integer
        description: The number of items matching your request in total for all pages.
        format: int32
      _links?:
        type: object
        properties:
          //:
            type: LinkObject
        description: Collection of links to related resources
      blocks?:
        type: array
        description: List of all blocks matching the given filter criteria.
        items:
          type: Block
  BookingListItem:
    type: object
    properties:
      _links?:
        type: object
        properties:
          //:
            type: LinkObject
        description: Collection of links to related resources
      confirmation_id?:
        type: string
        description: "The confirmation id for the booking which the guest can use\
          \ to check in on the kiosk, add the\r\n            booking to the mobile\
          \ app etc. It is used as identifier for all reservations done with the same\r\
          \n            booking request."
      reservations?:
        type: array
        description: Collection of reservations made with on booking request.
        items:
          type: BookingListReservationItem
  BookingListRequest:
    type: object
    properties:
      blockCode?:
        type: string
        description: Return all bookings where the block code matches the specified value.
      cancellationId?:
        type: string
        description: Return bookings for this cancellation id.
      channelCodes?:
        type: array
        description: Return all bookings where the channel code is one of the specified values.
        items:
          type: string
      companyEmail?:
        type: string
        description: "Return all bookings where the primary email address of the company\
          \ or the travel agent profile contains the specified value. The search is\
          \ executed case insensitive\r\n            and also stripping of any whitespaces."
      companyId?:
        type: string
        description: Return all bookings the id of the company or travel agent profile matches the specified value.
      companyName?:
        type: string
        description: "Return all bookings where the name of the linked company or\
          \ travel agent profile contains the specified value. The search is executed\
          \ case insensitive\r\n            and also stripping of any whitespaces."
      customerEmail?:
        type: string
        description: "Return all bookings where the primary email address of one of\
          \ the guests or the contact contains the specified value. The search is\
          \ executed case insensitive\r\n            and also stripping of any whitespaces."
      customerId?:
        type: string
        description: Return all bookings the id of one of the guests or the contact matches the specified value.
      customerName?:
        type: string
        description: "Return all bookings where the first or lastname of one of the\
          \ guests or the contact contains the specified value. The search is executed\
          \ case insensitive\r\n            and also stripping of any whitespaces."
      dateFilter?:
        type: string
        description: "Select a date field you want to filter bookings by. Only one\
          \ filter at a time can be applied. The to and from dates\r\n           \
          \ will then define the time range."
        enum:
        - ArrivalDate
        - DepartureDate
        - StayDate
        - CreationDate
        - ModificationDate
      externalId?:
        type: string
        description: Return all bookings exactly matching the specified external id. This filter is case sensitive.
      from?:
        type: datetime
        description: "Start date for the selected date filter. If you select arrival\
          \ date as date filter the bookings returned will have at least\r\n     \
          \       one reservation arriving on the specified date or later."
      hotelId?:
        type: integer
        description: Only return bookings for this specific hotel.
        format: int32
      marketCodes?:
        type: array
        description: Return all bookings where the market code is one of the specified values.
        items:
          type: string
      ratePlanCodes?:
        type: array
        description: Return all bookings where the rate plan code is one of the specified values.
        items:
          type: string
      reservationNumber?:
        type: integer
        description: "Return bookings matching this reservation number. Please note\
          \ that reservation numbers are only unique within a hotel. If you\r\n  \
          \          donÂ´t specify a hotel filter at the same time you could get\
          \ back multiple bookings from different hotels."
        format: int32
      reservationStatuses?:
        type: array
        description: Return all bookings where the reservation status is one of the specified values.
        items:
          type: string
          enum:
          - Tentative
          - Waitlisted
          - OnRequest
          - NonGuaranteed
          - Guaranteed
          - InHouse
          - CheckedOut
          - NoShow
          - Denied
          - Cancelled
          - Released
          - Walked
          - Expired
          - WalkIn
          - Registered
      roomNumber?:
        type: string
        description: Return all bookings having the specified room number assigned.
      roomTypes?:
        type: array
        description: Return all bookings where the room type is one of the specified values.
        items:
          type: string
      subChannelCodes?:
        type: array
        description: Return all bookings where the subchannel code is one of the specified values.
        items:
          type: string
      to?:
        type: datetime
        description: "End date for the selected date filter. If you select arrival\
          \ date as date filter the bookings returned will have at least\r\n     \
          \       one reservation arriving on the specified date or earlier."
  BookingListReservationItem:
    type: object
    properties:
      _links?:
        type: object
        properties:
          //:
            type: LinkObject
        description: Collection of links to related resources
      addon_services?:
        type: array
        description: "A list of addon service codes that are currently booked on the\
          \ reservation. Services which are charged\r\n            once per stay are\
          \ only visible before and on the day they are booked for"
        items:
          type: string
      adults?:
        type: integer
        description: The number of adults per room
        format: int32
      arrival_date?:
        type: datetime
        description: The arrival date of the guests
      balance?:
        type: number
        description: "The balance for all folios of this reservartion. It is calculated\
          \ by all already charged room and service rates plus manual charges \r\n\
          \            or charges from other systems like POS minus all payments.\
          \ A negative balance indicates the reservation is overpaid, a positive balance\
          \ \r\n            that the guest owes the hotel money"
        format: double
      block_code?:
        type: string
        description: If this reservation is a pick-up from a block you will see the appropriate block code here
      cancellation_id?:
        type: string
        description: The cancellation id if the reservation has been cancelled
      channel_code?:
        type: string
        description: "The code of the channel that was used when the booking has been\
          \ created. It is also known as source. \r\n            Possible values can\
          \ be OTA, GDS or DIRECT, but it is configurable per hotel"
      companies?:
        type: CompaniesInfo
        description: Information about related companies
      customers?:
        type: CustomersInfo
        description: Information about customers
      departure_date?:
        type: datetime
        description: The departure date of the guests
      external_id?:
        type: string
        description: "The external id for this reservation is the unique identifier\
          \ from the system that created the booking in hetras. It could be the id\
          \ of an\r\n            OTA like Expedia or booking.com or one of the GDS\
          \ systems like Amadeus or Galileo"
      hotel?:
        type: HotelInfo
        description: Information about the hotel the reservation belongs to
      market_code?:
        type: string
        description: The code of the market segment the rate plan for this reservation is linked to
      rate_plan?:
        type: RatePlan
        description: The rate plan details this reservation is related to
      reservation_number?:
        type: integer
        description: The reservation number of the reservation
        format: int32
      reservation_status?:
        type: string
        description: The current status of this reservation
        enum:
        - Tentative
        - Waitlisted
        - OnRequest
        - NonGuaranteed
        - Guaranteed
        - InHouse
        - CheckedOut
        - NoShow
        - Denied
        - Cancelled
        - Released
        - Walked
        - Expired
        - WalkIn
        - Registered
      room?:
        type: RoomInfo
        description: "The room type this reservation is related to and if applicable\
          \ the already assigned specific room for \r\n            this reservation"
      rooms?:
        type: integer
        description: "The number of rooms this reservation is valid for. After a multi-room\
          \ booking is done there will be \r\n            one reservation in hetras\
          \ for this booking for all rooms. The hotel staff then will split this reservation\
          \ into\r\n            one reservation per room to be able to check in the\
          \ guests"
        format: int32
      services?:
        type: array
        description: A list of details for all services included and addon service booked on this reservation
        items:
          type: Service
      subchannel_code?:
        type: string
        description: "The code of the subchannel that was used when the booking has\
          \ been created. Possible values can be \r\n            BOOKING, EXPEDIA\
          \ or WALKIN, but it is configurable per hotel"
  BookingListResponse:
    type: object
    properties:
      _count?:
        type: integer
        description: The number of items matching your request in total for all pages.
        format: int32
      _links?:
        type: object
        properties:
          //:
            type: LinkObject
        description: Collection of links to related resources
      bookings?:
        type: array
        description: Collection of bookings that match the defined filter criteria.
        items:
          type: BookingListItem
  CancellationPolicy:
    type: object
    properties:
      description?:
        type: string
        description: Description of the cancellation policy suitable for displaying to customers
      fee?:
        type: number
        description: The fee a guest has to pay when cancelling the reservation
        format: double
      fee_date?:
        type: datetime
        description: The date and time in UTC from when the fee has to be paid in case of cancellation
  CancellationResponse:
    type: object
    properties:
      _warnings?:
        type: array
        description: "Warnings that came up when your request was processed. Your\
          \ request will still be processed successfull when\r\n            you see\
          \ such warnings in your response"
        items:
          type: string
      balance?:
        type: number
        description: The current balance on the reservations folio without the cancellation fee
        format: double
      cancellation_fee?:
        type: number
        description: "The fee that might be charged to the folio of the reservation.\
          \ The cancelled reservation will\r\n            show up in the cancellation\
          \ and no show processing screen and the hotel staff will either charge\r\
          \n            the fee or waive it."
        format: double
      cancellation_id?:
        type: string
        description: The id of the successful cancellation. With this id the hotel staff will be able to find the reservation
    description: Result of cancel operation
  Card:
    type: object
    properties:
      id?:
        type: string
  CompaniesInfo:
    type: object
    properties:
      company?:
        type: Company
        description: The company linked to this reservation
      travel_agent?:
        type: Company
        description: The travel agent linked to this reservation
